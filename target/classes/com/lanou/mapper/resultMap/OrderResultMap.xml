<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="OrderResultMap">

	<resultMap type="Order" id="orderResult">
		<id column="rec_id" property="recId" javaType="int" />
		<result column="order_id" property="orderId" javaType="int" />
		<result column="goods_id" property="goodsId" javaType="int" />
		<result column="parent_id" property="parentId" javaType="int" />
		<result column="goods_name" property="goodsName" javaType="String" />
		<result column="shop_price" property="shopPrice" javaType="double" />
		<result column="goods_number" property="goodsNumber" javaType="int" />
	</resultMap>

</mapper>