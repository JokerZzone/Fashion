<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="AddressResultMap">

  	<resultMap type="Address" id="AddressResult">
  		<id column="address_id" property="addressId" javaType="int"/>
  		<result column="address_name" property="fullname" javaType="String"/>
  		<result column="tel" property="telephone" javaType="String"/>
  		<result column="mobile" property="mobilephone" javaType="String"/>
  		<result column="email" property="email" javaType="String"/>
  		<result column="address" property="address" javaType="String"/>
  		<result column="zipcode" property="zipcode" javaType="String"/>
  		<result column="user_id" property="user_id" javaType="int"/>
  		
  	</resultMap>
  	
</mapper>