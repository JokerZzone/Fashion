<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="CityResultMap">

	<resultMap type="City" id="cityResult">
		<id column="city_id" property="cityId" javaType="int" />
		<result column="city_name" property="cityName" javaType="String" />
		<result column="city_code" property="cityCode" javaType="String" />
		<result column="parent_id" property="parentId" javaType="int" />
	</resultMap>

</mapper>